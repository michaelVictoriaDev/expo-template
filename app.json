{
  "expo": {
    "name": "PayGPWA",
    "description": "PayGPWA",
    "slug": "paygpwa",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "3.0.3",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "ldpi": "./assets/splash/splash_LDPI.png",
      "mdpi": "./assets/splash/splash_MDPI.png",
      "hdpi": "./assets/splash/splash_HDPI.png",
      "xhdpi": "./assets/splash/splash_XHDPI.png",
      "xxhdpi": "./assets/splash/splash.png_XXHDPI",
      "xxxhdpi": "./assets/splash/splash.png_XXXHDPI",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "usesIcloudStorage": true,
      "bundleIdentifier": "com.xtendly.paygpwa",
      "buildNumber": "1",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "package": "com.xtendly.paygpwa",
      "permissions": [
        "WAKE_LOCK"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#3F71D7"
    }
  }
}
